<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="tickit_dashboard.dashboard_navigation" class="Tickit\DashboardBundle\Listener\NavigationBuilderListener">
            <argument type="service" id="router" />
            <tag name="kernel.event_listener" event="tickit.event.main_navigation_build" method="onBuild"/>
        </service>
    </services>
</container>
