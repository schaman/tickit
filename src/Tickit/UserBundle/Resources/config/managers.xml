<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
        <services>

            <service id='tickit_user.manager' class='Tickit\UserBundle\Manager\UserManager'>
                <argument type="service" id="doctrine" />
                <argument type="service" id="tickit_user.user_event_dispatcher" />
                <argument type="service" id="fos_user.user_manager.default" />
                <argument type="service" id="tickit_permission.manager" />
            </service>

            <service id="tickit_user.group_manager" class="Tickit\UserBundle\Manager\GroupManager">
                <argument type="service" id="doctrine" />
                <argument type="service" id="tickit_user.group_event_dispatcher" />
                <argument type="service" id="tickit_permission.manager" />
            </service>

        </services>
</container>