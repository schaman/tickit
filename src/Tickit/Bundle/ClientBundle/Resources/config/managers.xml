<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="tickit_client.manager" class="Tickit\Bundle\ClientBundle\Manager\ClientManager">
            <argument type="service" id="tickit_client.client_repository" />
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="tickit_client.client_event_dispatcher" />
        </service>

    </services>

</container>