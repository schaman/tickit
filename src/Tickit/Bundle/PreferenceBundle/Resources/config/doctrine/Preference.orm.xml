<?xml version="1.0" encoding="UTF-8" ?>

<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Tickit\Component\Preference\Model\Preference" table="preferences" repository-class="Tickit\Bundle\PreferenceBundle\Doctrine\Repository\PreferenceRepository">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="name" type="string" length="100" />
        <field name="systemName" column="system_name" type="string" length="100" />
        <field name="defaultValue" column="default_value" type="string" length="250" />
        <field name="type" type="string" length="8" />
    </entity>
</doctrine-mapping>
