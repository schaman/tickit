<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="tickit_team.manager" class="Tickit\TeamBundle\Manager\TeamManager">
            <argument type="service" id="doctrine" />
            <argument type="service" id="tickit_team.team_event_dispatcher" />
        </service>

        <service id="tickit_team.team_event_dispatcher" class="Tickit\TeamBundle\Event\Dispatcher\TeamEventDispatcher">
            <argument type="service" id="event_dispatcher" />
        </service>
    </services>

</container>